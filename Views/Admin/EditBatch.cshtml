@model SchoolManagementSystem.Models.Batch
@{
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
<script>
    $(document).ready(function () {

        $(Back).click(function () {
            window.location.href = "/Admin/ListAllBatch";
        });
    });
</script>

@section PageTitle{
<div class="page-title">
    <div>
        <h1>Edit Batch </h1>
    </div>
</div>
    }
<div class="box">
    <div class="box-title">
        <h3>Batch Information</h3>
    </div>
    <div class="box-content">
        <form>
            <div class="row-fluid"><br />
                <div class="span4">
                    <label for="subject_name">Name</label>
                    <input class="form-control input-xlarge" required="required" autofocus="autofocus" type="text" name="Name" id="subject_name" value="@Model.Name">
                </div>
            </div><br />
            <div class="form-actions">
                <div class="row-fluid">
                    <div class="span3 pull-right">
                        <input id="Save" type="submit" name="Save" value="Save" class=" btn btn-block btn-success form-control col-md-4">
                    </div>
                    <div class="span2">
                        <input id="Back" type="button" name="Back" value="back" class="btn btn-block btn-success form-control col-md-4">
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>