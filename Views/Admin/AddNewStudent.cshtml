<script>
    $(document).ready(function () {
        $('#getParentBtn').click(function () {
            var studentId = $('#getParent').val();
            $.ajax({
                url: "/Admin/GetParent/?studentId=" + studentId,
                dataType: 'JSON',
                type: 'GET',
                success: function (result) {
                    $('[name="FatherName"]').val(result.FatherName);
                    $('[name="FatherEmail"]').val(result.FatherEmail);
                    $('[name="FatherIqamaNumber"]').val(result.FatherIqamaNumber);
                    $('[name="FatherIqamaExpiry"]').val(result.FatherIqamaExpiry);
                    $('[name="FatherMobile"]').val(result.FatherMobile);
                    $('[name="MotherEmail"]').val(result.MotherEmail);
                    $('[name="MotherMobile"]').val(result.MotherMobile);
                    $('[name="MotherName"]').val(result.MotherName);
                    newParent();
                },
                error: function () {
                    alert('error');
                }
            });
        });
    });
    function parentAlreadyExits() {
        $("#parentExistsForm").show();
        $("#newParentForm").hide();
    }

    function newParent() {
        $("#parentExistsForm").hide();
        $("#newParentForm").show();
    }
</script>







