}

@section Scripts{
    <script src="~/Content/assets/bootstrap-wizard/jquery.bootstrap.wizard.min.js"></script>
    <link href="~/Content/assets/bootstrap-datepicker/css/datepicker.css" rel="stylesheet" />
    <link href="~/Content/assets/bootstrap-fileupload/bootstrap-fileupload.css" rel="stylesheet" />
    <script src="~/Content/assets/bootstrap-datepicker/js/bootstrap-datepicker.js"></script>
    <script src="~/Scripts/Custom/Std001.js"></script>
    <script src="~/Content/assets/bootstrap-fileupload/bootstrap-fileupload.min.js"></script>
    @*<script src="~/Content/assets/jquery-validation/dist/additional-methods.min.js"></script>
        <script src="~/Content/assets/jquery-validation/dist/jquery.validate.min.js"></script>*@
}

<div id="modalCSV" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel2" aria-hidden="true">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
        <h3 id="myModalLabel2">Uploading CSV</h3>
    </div>
    <form action="/Admin/AddStudentCSV" method="post" enctype="multipart/form-data">
        <div class="modal-body">
            <p>Select a CSV File</p><br />
            <input type="file" name="File" id="File"/>
        </div>
        <div class="modal-footer">
            <button class="btn" data-dismiss="modal" aria-hidden="true">Cancel</button>
            <button class="btn btn-primary" type="submit">Upload</button>
        </div>
    </form>
</div>

@section PageTitle{
    <div class="page-title">
        <div>
            <h1><i class="icon-file-alt"></i>Add Student</h1>
        </div>
    </div>
}
